{
  "name": "copilot-chat-analyzer",
  "private": false,
  "version": "0.0.1",
  "description": "Simple library for analyzing exported GitHub Copilot chats",
  "keywords": [
    "copilot",
    "chat",
    "analyzer",
    "github",
    "typescript"
  ],
  "author": "dealenx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dealenx/copilot-chat-analyzer.git"
  },
  "bugs": {
    "url": "https://github.com/dealenx/copilot-chat-analyzer/issues"
  },
  "homepage": "https://github.com/dealenx/copilot-chat-analyzer#readme",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.4",
    "typescript": "~5.8.3",
    "vite": "^7.1.7"
  },
  "dependencies": {
    "tsup": "^8.5.0"
  }
}
